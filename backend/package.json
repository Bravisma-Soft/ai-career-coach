{
  "name": "ai-career-coach-backend",
  "version": "1.0.0",
  "description": "AI Career Coach Backend - Intelligent career guidance platform powered by AI",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child -r tsconfig-paths/register src/server.ts",
    "worker:resume": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/jobs/workers/resume-parse.worker.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:migrate": "npx prisma migrate deploy && npm run start:prod",
    "start:worker": "NODE_ENV=production node dist/jobs/workers/resume-parse.worker.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "NODE_ENV=test jest --watch --runInBand",
    "test:coverage": "NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
    "test:ai": "bash scripts/run-ai-tests.sh",
    "test:ai:analyze": "node scripts/analyze-ai-tests.js",
    "test:ai:html": "node scripts/generate-html-report.js",
    "test:ai:csv": "node scripts/generate-csv-report.js",
    "test:ai:reports": "npm run test:ai:html && npm run test:ai:csv && npm run test:ai:analyze",
    "test:integration": "NODE_ENV=test jest --testMatch='**/tests/integration/**/*.test.ts' --runInBand",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "db:push": "prisma db push",
    "db:backup": "bash scripts/db-backup.sh",
    "db:restore": "bash scripts/db-restore.sh",
    "db:migrate-helper": "bash scripts/migrate.sh",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "career-coach",
    "anthropic",
    "claude",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@prisma/client": "^6.17.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/cheerio": "^0.22.35",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.25.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.37.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prisma": "^6.17.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
